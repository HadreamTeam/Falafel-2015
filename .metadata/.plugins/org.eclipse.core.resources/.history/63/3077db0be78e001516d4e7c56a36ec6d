	}
    
    public TwoMotorPID(Victor rear, Victor front, double p, double i, double d) {
		super(p, i, d, 0.02);
		rearMotor = rear;
		frontMotor = front;
		// TODO Auto-generated constructor stub
	}

	public void initDefaultCommand() {
